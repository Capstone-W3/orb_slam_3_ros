<launch>
    <node pkg="orb_slam3" type="Stereo" name="orb_slam3" args="$(find orb_slam3_lib)/Vocabulary/ORBvoc.txt $(find orb_slam3)/settings/realsense_stereo.yaml False" cwd="node" output="screen">
        <remap from="/camera/left/image_raw" to="/camera/ir/image_raw" />
        <remap from="/camera/right/image_raw" to="/camera/ir2/image_raw" />
    </node>
</launch>

